extends layout

block sidebar
    ul(class='sidebar-nav')
        li
            a(href='/') Home
        li
            a(href='/orders/create') Create order


block content
    h2= message
    ul
        each el in val
            a(href='/orders/' + el.idclientOrder)
                li= 'id: ' +  el.idclientOrder + ': clientId: ' + el.clientId + ' cookId:  ' +  el.cookId + ' orderDone: ' + el.orderDone + ' timeCooking: ' + el.timeCooking